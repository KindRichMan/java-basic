package heapmemory;

public class StrintComparison {

	public static void main(String[] args) {
		String str1 = "Hello";
		String str2 = "Hello";
		String str3 = "Hallo";
		System.out.println(str1.hashCode());
		System.out.println(str2.hashCode());
		System.out.println(str3.hashCode());
		//문자열은 일부분만 변경이 불가능함.
		//str1[0];
	}

}
